<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="pt.ipp.isep.dei.esoft.project.application.controller.GUIController.ListGreenSpacesControllerGUI">

    <children>
        <ChoiceBox fx:id="sortingAlgorithmChoiceBox" VBox.vgrow="NEVER" prefWidth="200" />
        <ListView fx:id="greenSpaceListView" VBox.vgrow="ALWAYS" prefHeight="300" prefWidth="400"/>
        <Button text="Back" onAction="#handleBack" prefWidth="200"/>
        <Button text="Update Sorting Algorithm" onAction="#updateSortingAlgorithm" prefWidth="200"/>
    </children>
</VBox>
