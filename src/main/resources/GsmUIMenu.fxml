<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Label?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="pt.ipp.isep.dei.esoft.project.application.controller.GUIController.GsmUIMenuControllerGUI" spacing="20">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>
    <children>
        <Label fx:id="emailLabel" text="Logged in as:"/>
        <!-- Other UI elements -->
    </children>
    <Button text="Register Green Space" onAction="#handleRegisterGreenSpace" prefWidth="200" style="-fx-background-color: #007bff; -fx-text-fill: white;"/>
    <Button text="View Green Spaces" onAction="#handleListGreenSpaces" prefWidth="200" style="-fx-background-color: #28a745; -fx-text-fill: white;"/>
</VBox>
